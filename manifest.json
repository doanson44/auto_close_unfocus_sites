{
    "name": "Blocked site",
    "version": "2.0",
    "permissions": [
        "webNavigation",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_title": "Blocked site"
    },
    "manifest_version": 2
  }